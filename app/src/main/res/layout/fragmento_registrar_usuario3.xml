<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    tools:context=".frontend.usuario.fragmento.FragmentoRegistrarUsuario3">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/consLyt_parentPrincipal_registro_3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bkgd_esquinas_superiores_redondeadas">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline1_textView_editView_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.10" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline1_button_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.20" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline1_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.203" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.233" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.3413" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline4_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.3713" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline5_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.4796" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline6_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5096" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline7_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.6179" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline8_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.6479" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline9_editView_codigo_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.7562" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2_button_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.79" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2_textView_editView_registro_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.90" />

        <Button
            android:id="@+id/button_regresar_registro_3"
            style="@style/BtnRegresar1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@id/guideline1_button_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_textView_editView_registro_3"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.06"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/textView_titulo_registro_3"
            style="@style/TxtViewBlanco"
            android:text="@string/titulo_registro"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="5:1"
            app:layout_constraintEnd_toEndOf="@id/guideline2_textView_editView_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_textView_editView_registro_3"
            app:layout_constraintTop_toTopOf="@id/button_regresar_registro_3"
            app:layout_constraintVertical_bias="0.15" />

        <TextView
            android:id="@+id/textView_instrucciones1_registro_3"
            style="@style/TxtViewAzulOscuro"
            android:text="@string/instrucciones1_registro3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="5:1"
            app:layout_constraintEnd_toEndOf="@id/guideline2_textView_editView_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_textView_editView_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_titulo_registro_3"
            app:layout_constraintVertical_bias="0.05" />


        <TextView
            android:id="@+id/textView_instrucciones2_registro_3"
            style="@style/TxtViewTransparente"
            android:text="@string/instrucciones2_registro3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="4:1"
            app:layout_constraintEnd_toEndOf="@id/guideline2_textView_editView_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_textView_editView_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones1_registro_3"
            app:layout_constraintVertical_bias="0.05" />

        <View
            android:id="@+id/separador_registro_3"
            style="@style/ViewSeparador"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="70:1"
            app:layout_constraintEnd_toEndOf="@id/guideline2_textView_editView_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_textView_editView_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones2_registro_3"
            app:layout_constraintVertical_bias="0.05"/>

        <TextView
            android:id="@+id/textView_instrucciones3_registro_3"
            style="@style/TxtViewTransparente"
            android:text="@string/instrucciones3_registro3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="4:1"
            app:layout_constraintEnd_toEndOf="@id/guideline2_textView_editView_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_textView_editView_registro_3"
            app:layout_constraintTop_toBottomOf="@id/separador_registro_3"
            app:layout_constraintVertical_bias="0.05" />

        <EditText
            android:id="@+id/editText1_codigo_registro_3"
            style="@style/EditTextCodigo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="19:20"
            app:layout_constraintEnd_toEndOf="@id/guideline1_editView_codigo_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_textView_editView_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones3_registro_3"
            app:layout_constraintVertical_bias="0.1"
            tools:ignore="LabelFor"/>

        <EditText
            android:id="@+id/editText2_codigo_registro_3"
            style="@style/EditTextCodigo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@id/guideline3_editView_codigo_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline2_editView_codigo_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones3_registro_3"
            app:layout_constraintVertical_bias="0.1"
            tools:ignore="LabelFor"/>

        <EditText
            android:id="@+id/editText3_codigo_registro_3"
            style="@style/EditTextCodigo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@id/guideline5_editView_codigo_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline4_editView_codigo_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones3_registro_3"
            app:layout_constraintVertical_bias="0.1"
            tools:ignore="LabelFor"/>

        <EditText
            android:id="@+id/editText4_codigo_registro_3"
            style="@style/EditTextCodigo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@id/guideline7_editView_codigo_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline6_editView_codigo_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones3_registro_3"
            app:layout_constraintVertical_bias="0.1"
            tools:ignore="LabelFor"/>

        <EditText
            android:id="@+id/editText5_codigo_registro_3"
            style="@style/EditTextCodigo"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@id/guideline9_editView_codigo_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline8_editView_codigo_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones3_registro_3"
            app:layout_constraintVertical_bias="0.1"
            tools:ignore="LabelFor"/>

        <EditText
            android:id="@+id/editText6_codigo_registro_3"
            style="@style/EditTextCodigo"
            android:imeOptions="normal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintEnd_toEndOf="@id/guideline2_textView_editView_registro_3"
            app:layout_constraintStart_toStartOf="@+id/guideline2_button_registro_3"
            app:layout_constraintTop_toBottomOf="@id/textView_instrucciones3_registro_3"
            app:layout_constraintVertical_bias="0.1"
            tools:ignore="LabelFor" />

        <Button
            android:id="@+id/button_reenviar_codigo_registro_3"
            style="@style/TxtViewTransparente"
            android:text="@string/reenviarCodigoBtn"
            android:textAllCaps="false"
            android:textSize="@dimen/text_size_parrafo_small"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="7:1"
            app:layout_constraintEnd_toEndOf="@id/guideline2_button_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_button_registro_3"
            app:layout_constraintTop_toBottomOf="@id/editText1_codigo_registro_3"
            app:layout_constraintVertical_bias="0.1" />

        <Button
            android:id="@+id/button_verificar_registro_3"
            style="@style/BtnMoradoOscuro"
            android:text="@string/verificarBtn"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@id/guideline2_button_registro_3"
            app:layout_constraintStart_toStartOf="@id/guideline1_button_registro_3"
            app:layout_constraintTop_toBottomOf="@+id/button_reenviar_codigo_registro_3"
            app:layout_constraintVertical_bias="0.1" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>